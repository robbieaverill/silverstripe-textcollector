{
    "name": "silverstripe/textcollector",
    "description": "Collects translatable strings from SilverStripe classes and templates",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "require": {
        "php": ">=7.1.0",
        "silverstripe/framework": "^4.0",
        "nikic/php-parser": "^4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "SilverStripe\\TextCollector\\": "src/",
            "SilverStripe\\TextCollector\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
